<%= form_for @guest do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :locator %><br />
    <%= f.text_field :locator %>
  </p>
	<p>
    <%= f.label :event_date %><br />
    <%= f.text_field :event_date %>
  </p>
  <p><%= f.submit %></p>
<% end %>
